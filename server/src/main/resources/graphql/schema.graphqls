type Query{
    getUserLoginInfo(userName: String): UserLoginInfo
}

type Mutation{
    addUser(userLoginInfo: UserLoginInfoIp): Boolean
}


input UserLoginInfoIp{
    name: String
    email: String
    phoneNo: Int
    password: String
}

type UserLoginInfo{
    name: String!
    email: String!
    phoneNo: Int!
    password: String!
}
