type Query{
    getUserLoginInfo(userName: String): UserLoginInfo
}

type Mutation{
    addUser(userLoginInfo: UserLoginInfoIp): Boolean
}


input UserLoginInfoIp{
    firstName: String
    lastName: String
    email: String
    phoneNo: Int
    password: String
}

type UserLoginInfo{
    firstName: String!
    lastName: String!
    email: String!
    phoneNo: Int!
    password: String!
}
